				SaveOption="SaveCurrent"
				RunFromDir="%rw">
				<Exec CmdLine="mcs -debug -target:module -out:%bd%n.obj %f"/>
			</Target>
			<Target
				Name="Build"
				Dialog="_mono_options_form Compiler"
				MenuCaption="&amp;Build"
				CaptureOutputWith="ProcessBuffer"
				SaveOption="SaveWorkspaceFiles"
